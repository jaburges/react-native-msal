{
  "name": "@jaburges/react-native-msal",
  "version": "4.0.3-custom.1",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "description": "React Native wrapper for iOS and Android",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "source": "src/index",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "expo-module": "expo-module",
    "lint": "expo-module lint",
    "prepare": "expo-module prepare && husky install",
    "prepublishOnly": "expo-module prepublishOnly",
    "test": "expo-module test",
    "example": "yarn --cwd example",
    "pods": "cd example && npx pod-install",
    "bootstrap": "yarn example && yarn && yarn pods",
    "predeploy": "yarn example build:web",
    "deploy": "gh-pages -d example/web-build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "msal",
    "azure",
    "b2c",
    "aad",
    "expo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaburges/react-native-msal.git"
  },
  "author": "Stash Energy <dev@stash.energy> (https://stash.energy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaburges/react-native-msal/issues"
  },
  "homepage": "https://github.com/jaburges/react-native-msal#readme",
  "dependencies": {
    "@azure/msal-browser": "^2.1.0",
    "@expo/config-plugins": "^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expo-module-scripts": "^3.0.0",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "semantic-release": "^22.0.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-native": ">=0.73.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "printWidth": 120
  }
}